{
  "name": "@devsisters/react-pixi",
  "version": "1.0.2",
  "description": "Write PixiJS applications using React declarative style.",
  "main": "./dist/index.js",
  "umd:main": "umd.js",
  "module": "./dist/index.mjs",
  "jsnext:module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "author": "Patrick Brouwer",
  "license": "MIT",
  "contributors": [
    "Devsisters Corp."
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inlet/react-pixi.git"
  },
  "files": [
    "dist/",
    "LICENSE",
    "index.js",
    "index.d.ts",
    "umd.js"
  ],
  "scripts": {
    "docz:dev": "rimraf ./docs && docz dev",
    "docz:build": "rimraf ./docs && docz build",
    "eslint": "eslint .",
    "build:umd": "rollup -c",
    "build:umd:watch": "rollup -c --watch",
    "build:cjs": "babel src --env-name cjs --out-dir dist --source-maps",
    "build:cjs:watch": "yarn build:cjs --watch",
    "build:esm": "babel src --env-name esm --out-dir dist --out-file-extension .mjs --source-maps",
    "build:esm:watch": "yarn build:esm --watch",
    "build:dev": "cross-env NODE_ENV=development concurrently \"yarn:build:umd\" \"yarn:build:cjs\" \"yarn:build:esm\"",
    "build:dev:watch": "cross-env NODE_ENV=development concurrently \"yarn:build:umd:watch\" \"yarn:build:cjs:watch\" \"yarn:build:esm:watch\"",
    "build:prod": "cross-env NODE_ENV=production concurrently \"yarn:build:umd\" \"yarn:build:cjs\" \"yarn:build:esm\"",
    "build": "rimraf ./dist && yarn build:dev && yarn build:prod",
    "test": "jest --silent",
    "test:watch": "jest --silent --watch",
    "prepare": "if-env CI=true && echo \"skip build\" || (npm run test && npm run eslint && npm run build)"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-reconciler": "0.18.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.8.0",
    "canvas": "^2.5.0",
    "concurrently": "^5.1.0",
    "cross-env": "^5.2.0",
    "docz": "^1.2.0",
    "docz-theme-default": "1.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "if-env": "^1.0.4",
    "jest": "^24.8.0",
    "jest-webgl-canvas-mock": "^0.2.2",
    "pixi.js": "5.0.4",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "pixi.js": "^5.x.x",
    "prop-types": "^15.x.x",
    "react": "^16.x.x",
    "react-dom": "^16.x.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "fbjs": "^1.0.0",
    "idx": "^2.5.6",
    "lodash": "^4.17.15",
    "performance-now": "^2.1.0",
    "react-reconciler": "0.21.0-alpha.0"
  },
  "resolutions": {
    "serialize-javascript": "^2.1.1"
  },
  "jest": {
    "collectCoverage": false,
    "setupFiles": [
      "jest-webgl-canvas-mock",
      "./test/bootstrap.js"
    ]
  }
}
